/* Test arithmetic and comparisons. */

int main() {
    int x = readInt();
    int y = -23;
    printInt(x+y);
    printInt(x-y);
    printInt(x*y);
    printInt(45/2);
    printInt(78%0);
    printBool(x-y > x+y);
    printBool(x/y <= x*y);
    printString("string"+" "+"concatenation");
    return 0 ;
}

void printBool(boolean b) {
  if (b) {
    printString("true");
    return;
  } else {
    printString("false");
    return;
 }
}